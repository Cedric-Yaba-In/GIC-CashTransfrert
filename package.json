{
  "name": "gic-cashtransfer",
  "version": "1.0.0",
  "description": "GIC CashTransfer - Money transfer application by GIC Promote LTD",
  "private": true,
  "scripts": {
    "dev": "npm run db:push && npm run db:seed && next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:init": "npm run db:push && npm run db:seed",
    "flutterwave:init": "tsx scripts/init-flutterwave-methods.ts",
    "flutterwave:migrate": "tsx scripts/migrate-flutterwave-config.ts",
    "flutterwave:sync-banks": "node -e \"console.log('Use the API endpoint /api/flutterwave/sync-banks to sync banks for a specific country')\"",
    "rates:init": "tsx scripts/init-transfer-rates.ts",
    "wallets:check": "tsx scripts/check-wallets.ts"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@prisma/client": "^5.0.0",
    "prisma": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "twilio": "^4.19.0",
    "axios": "^1.6.0",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "react-hook-form": "^7.48.0",
    "react-select": "^5.8.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.8.0",
    "lucide-react": "^0.292.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "next-intl": "^3.0.0",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "tsx": "^4.6.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}